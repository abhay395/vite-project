{
    "builds": [
      {
        "src": "main.jsx",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "dist/$1"
      }
    ]
  }